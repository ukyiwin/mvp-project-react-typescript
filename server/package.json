{
  "name": "uniserver",
  "version": "1.0.0",
  "homepage": ".",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "push": "git push origin && now && now alias uniserver.now.sh"
  },
  "dependencies": {
    "azure": "^2.2.1-preview",
    "bcryptjs": "2.4.3",
    "cloudinary": "^1.10.0",
    "cors": "^2.8.4",
    "feedparser": "^2.2.9",
    "graphql-yoga": "1.3.4",
    "jsonwebtoken": "8.1.1",
    "mime-types": "^2.1.18",
    "multiparty": "^4.1.3",
    "node-cron": "^1.2.1",
    "prisma-binding": "1.5.12",
    "rss-parser": "^3.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.14.3",
    "nodemon": "1.15.1",
    "npm-run-all": "4.1.2",
    "prisma": "1.2.6",
    "rimraf": "2.6.2",
    "ts-node": "5.0.0",
    "typescript": "2.7.2"
  }
}
